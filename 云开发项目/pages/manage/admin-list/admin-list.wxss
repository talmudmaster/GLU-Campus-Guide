/* pages/manage/admin-list/admin-list.wxss */
page {
  background: #f3f3f3;
}

.card {
  margin: 10px 15px;
  border-radius: 10px;
  background-color: white;
}

.card-container {
  margin-bottom: 30px;
  margin: 10px;
}

.card-container-title {
  display: flex;
  align-items: center;
  flex-direction: row;
  margin-bottom: 10px;
  padding-top: 10px;
}

.card-container-title-font {
  width: 100%;
  color: #73a5d6;
  margin: 15px 0;
  font-size: 30px;
  text-align: center;
}

.card-container-container {
  border-bottom: 10px solid white;
}

.card-container-container-title {
  margin: 10px 0 10px 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: larger;
}

.card-container-container-title-title {
  width: 80px;
  height: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.title {
  border-bottom: 1px solid grey;
}

.card-container-container-site-title {
  width: 160px;
  height: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: large;
  text-align: center;
  flex-direction: column;
}
